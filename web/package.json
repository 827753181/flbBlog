{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "babel-plugin-import": "^1.13.1"
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@umijs/fabric": "^2.2.2",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.27",
    "ahooks": "^2.8.1",
    "clsx": "^1.1.1",
    "cross-env": "^7.0.2",
    "dayjs": "^1.9.4",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.20",
    "number-precision": "^1.5.0",
    "prettier": "^1.19.1",
    "qs": "^6.9.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.27",
    "unstated-next": "^1.1.0",
    "yorkie": "^2.0.0"
  }
}
